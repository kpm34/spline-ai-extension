{
  "name": "spline-cli-editor",
  "version": "0.1.0",
  "description": "A command-line interface for editing inside Spline",
  "main": "src/index.js",
  "bin": {
    "spline-edit": "./bin/spline-edit"
  },
  "scripts": {
    "start": "node src/index.js",
    "dev": "node --inspect src/index.js",
    "api-server": "node src/api-server.js",
    "api-server:dev": "node --inspect src/api-server.js",
    "test": "jest",
    "build": "npm run test",
    "lint": "eslint src/ --fix",
    "prepare": "chmod +x bin/spline-edit && chmod +x src/api-server.js"
  },
  "keywords": [
    "spline",
    "cli",
    "editor",
    "3d",
    "design",
    "command-line"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.67.0",
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@splinetool/runtime": "^1.10.84",
    "axios": "^1.3.4",
    "body-parser": "^2.2.0",
    "chalk": "^4.1.2",
    "commander": "^9.4.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "fs-extra": "^11.1.0",
    "imap-simple": "^5.1.0",
    "inquirer": "^8.2.5",
    "mailparser": "^3.7.5",
    "open": "^10.2.0",
    "openai": "^6.6.0",
    "puppeteer": "^24.26.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.3",
    "eslint": "^8.36.0",
    "jest": "^29.4.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/username/spline-cli-editor.git"
  },
  "bugs": {
    "url": "https://github.com/username/spline-cli-editor/issues"
  },
  "homepage": "https://github.com/username/spline-cli-editor#readme"
}
